      "cell_type": "code",
      "source": [
        "#importing Libraries\n",
        "from Bio import AlignIO, Phylo\n",
        "from Bio.Phylo.TreeConstruction import DistanceCalculator, DistanceTreeConstructor\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd\n"
      ],
      "metadata": {
        "id": "K6vtAzrvKS9t"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#FOR NUCLEOTIDE ALIGNMENT\n",
        "\n",
        "alignment_nt = AlignIO.read(\"aligned_nucleotide.aln\", \"clustal\")\n",
        "\n",
        "#distance matrix\n",
        "calculator_nt = DistanceCalculator(\"identity\")\n",
        "distance_matrix_nt = calculator_nt.get_distance(alignment_nt)\n"
      ],
      "metadata": {
        "id": "jhffGTJCKWJ8"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Heatmap for Nucleotide sequence\n",
        "#Converting matrix to DataFrame\n",
        "df_nt = pd.DataFrame(list(distance_matrix_nt),\n",
        "                     index=distance_matrix_nt.names,\n",
        "                     columns=distance_matrix_nt.names)\n",
        "\n",
        "# Plot heatmap\n",
        "plt.figure(figsize=(6, 5))\n",
        "sns.heatmap(df_nt, annot=True, cmap=\"YlGnBu\", cbar=True)\n",
        "plt.title(\"Nucleotide Sequence Identity Heatmap\")\n",
        "plt.show()"
      ],
      "metadata": {
